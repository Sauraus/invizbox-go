config wifi-device 'mt7603e'
    option type 'mt7603e'
    option channel '7'
    option disabled '0'

config wifi-iface 'ap'
    option device 'mt7603e'
    option network 'ap'
    option mode 'ap'
    option ifname 'ra0'
    option ssid 'InvizBox-Go-Setup'
    option encryption 'psk-mixed'
    option cipher 'ccmp'
    option key 'TOKENPASSWORD'
    option isolate '1'

config wifi-iface 'wan'
    option device 'mt7603e'
    option network 'wan'
    option mode 'sta'
    option ifname 'apcli0'
    option isolate '1'
    option disabled '1'
